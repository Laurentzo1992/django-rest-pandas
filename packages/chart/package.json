{
    "name": "@wq/chart",
    "version": "2.0.0-alpha.0",
    "description": "wq framework plugin providing re-usable d3.js charts for Django REST Pandas",
    "main": "dist/index.js",
    "files": [
        "src/",
        "dist/"
    ],
    "type": "module",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/chart",
        "prettier": "cd ../../ && npm run prettier",
        "lint": "cd ../../ && npm run lint",
        "build": "cd ../../ && npm run rollup -- -c packages/chart/rollup.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/django-rest-pandas.git",
        "directory": "packages/chart"
    },
    "keywords": [
        "wq",
        "chart",
        "django-rest-pandas"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/django-rest-pandas/issues"
    },
    "homepage": "https://django-rest-pandas.wq.io/@wq/chart",
    "dependencies": {
        "@wq/pandas": "^2.0.0-alpha.0",
        "d3": "^7.8.2",
        "mustache": "^4.2.0"
    }
}
